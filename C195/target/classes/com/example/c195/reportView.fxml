<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.c195.controller.ReportViewController" spacing="10" alignment="CENTER">
    <ComboBox fx:id="reportComboBox" onAction="#onGenerateReport"/>
    <TableView fx:id="reportTableView">
        <columns>
            <TableColumn fx:id="col1" text="Column 1"/>
            <TableColumn fx:id="col2" text="Column 2"/>
            <TableColumn fx:id="col3" text="Column 3"/>
            <TableColumn fx:id="col4" text="Column 4"/>
            <TableColumn fx:id="col5" text="Column 5"/>
            <TableColumn fx:id="col6" text="Column 6"/>
            <TableColumn fx:id="col7" text="Column 7"/>
        </columns>
    </TableView>
    <Button text="Back" onAction="#onBack"/>
</VBox>
